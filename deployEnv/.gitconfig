[push]
	default = simple
[filter "lfs"]
    clean = git-lfs clean -- %f
	smudge = git-lfs smudge --skip -- %f
	process = git-lfs filter-process --skip
	required = true
[credential]
	helper = cache
	helper = cache --timeout 36000
[core]
	autocrlf = input
